Sign up for a haircut boss!
<%= simple_form_for(@haircut) do |f| %>

	<%= f.label :review %>
	<%= f.input :review %>

	<%= f.hidden_field :barber_id, :value => Barber.first.id  %>
	<%= f.hidden_field :customer_id, :value => current_user.id  %>
	<%= f.input :created_at, as: :datetime, start_year: Date.today.year,
                              end_year: Date.today.year - 12,
                              discard_day: false,
                              order: [:month, :day, :year, :hour, :minute] %>


	<%= f.submit "Create this shit!", class: 'btn btn-primary' %>
<% end %>
<div id="calendar"></div>
<%#= signed_in? %> <br />

